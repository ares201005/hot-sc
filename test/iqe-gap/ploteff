a=0.3

set mxtics 2
set mytics 2

set format x "%3.1f"
set format y "%3.1f"

set xlabel "Bandgap (eV)"
set ylabel "ECE (%)"
p 'eff.dat' u 1:($1*$2*a) w lp lw 5 pt 7 ps 2 lc rgb "black" ti ""

#p 'eff.dat' u 1:($1*$2*a) w lp lw 5 pt 7 ps 2 lc rgb "black" ti "ECE"

pause -1

set term postscript eps color solid enhanced "Arial-Bold" 28
set output "ece-gap.eps"
replot
