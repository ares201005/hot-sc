set ylabel "Photocurrent (nA)"
set xlabel "Photon energy (eV)"

set format y ""
set format y2 ""

set mytics 2
set my2tics 2
set mxtics 2
set ytics nomirror
set y2tics

set xrange [0.7:4]

p 'curr-freq.dat' u 1:3 w l lw 5 lc rgb "red" ti "Hot-carrier SC" ,\
  '../pv-freq/curr-freq.dat' u 1:3 w l lw 5 lc rgb "blue"  axis x1y2 ti "Conventional SC"


pause -1

set term postscript eps color solid enhanced "Arial-Bold" 26
set output "curr-freq.eps"

replot
