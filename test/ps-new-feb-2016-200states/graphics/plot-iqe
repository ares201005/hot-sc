
set mxtics 2
set mytics 2

set xrange [0.5:4.5]

set lmargin at screen 0.15
set rmargin at screen 0.90
set bmargin at screen 0.17
set tmargin at screen 0.90

set label 1 "(d)" at screen 0.16,0.86
set key  at screen 0.45,0.88

set xlabel "Photon energy (eV)"
set ylabel "IQE (%)"

p 'flux-freq.dat' u 1:4 w l lw 5 lc rgb "black" ti "" 
#p 'flux-freq.dat' u 1:4 w l lw 5 lc rgb "black" ti "IQE" 

pause -1

set term postscript eps color solid enhanced "Arial-Bold" 28
set output "iqe.eps"

replot
