
set xlabel "Photon energy (eV)"
set ylabel "Current (uA)"

set xtics format "%3.1f"

set xrange [1.5:3.5]

set xtics 0.5
set mxtics 2

#set ytics 400 
set mytics 2

p 'curr-freq.dat' u 1:($3/1000) w l lw 5 lc rgb "black" ti ""

pause -1



set term postscript eps color solid enhanced "Arial-Bold" 26
set output "curr-freq.eps"

replot


set terminal x11

set yrange [0:105]
set ytics 20
set mytics 2
set mxtics 2

set ylabel "IQE (%)"

set xrange [1.5:3.5]

p 'flux.dat' u 1:3 w l lw 5 lc rgb "black" ti ""

pause -1


set term postscript eps color solid enhanced "Arial-Bold" 26
set output "iqe-pv-freq.eps"

replot
